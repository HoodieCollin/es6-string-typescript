{
  "name": "es6-string-typescript",
  "displayName": "es6-string-typescript",
  "description": "Highlight TS language in ES6 template literals",
  "version": "1.1.0",
  "publisher": "HoodieCollin",
  "icon": "docs/logo.png",
  "engines": {
    "vscode": "^1.19.0"
  },
  "galleryBanner": {
    "color": "#dedede",
    "theme": "light"
  },
  "repository": {
    "url": "https://github.com/hoodiecollin/es6-string-typescript"
  },
  "bugs": {
    "url": "https://github.com/hoodiecollin/es6-string-typescript/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "typescript",
    "es6",
    "lit-typescript",
    "lit-html"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.tsx"
        ],
        "scopeName": "inline.es6-typescript",
        "path": "./syntaxes/es6-inline-typescript.json",
        "embeddedLanguages": {
          "meta.embedded.block.typescript": "typescript"
        }
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.9.2"
  }
}
